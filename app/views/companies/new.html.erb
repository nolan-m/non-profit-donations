<h1>Add a Non Profit</h1>

<%= form_for @company do |f| %>
  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
   <div class="form-group">
    <%= f.label :short, "Provide a short one line description"%><br>
    <%= f.text_field :short %>
  </div>
   <div class="form-group">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <%= f.submit :class => 'btn btn-default' %>
<% end %>
